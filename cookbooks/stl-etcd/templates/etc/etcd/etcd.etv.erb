<% if node['fqdn'] == 'hzubernetes01.qa.tabajara.intranet' -%>
ETCD_DATA_DIR=/var/lib/etcd
ETCD_ADVERTISE_CLIENT_URLS=https://<%= node['etcd'][node.chef_environment]['srv01_ip'] %>:2379
ETCD_INITIAL_ADVERTISE_PEER_URLS=https://<%= node['etcd'][node.chef_environment]['srv01_ip'] %>:2380
ETCD_INITIAL_CLUSTER_STATE=new
ETCD_LISTEN_CLIENT_URLS=http://127.0.0.1:2379,https://<%= node['etcd'][node.chef_environment]['srv01_ip'] %>:2379
ETCD_ELECTION_TIMEOUT=5000
ETCD_HEARTBEAT_INTERVAL=250
ETCD_LISTEN_PEER_URLS=https://<%= node['etcd'][node.chef_environment]['srv01_ip'] %>:2380
ETCD_NAME=<%= node['etcd'][node.chef_environment]['srv01_host'] %>
ETCD_PROXY=off
ETCD_INITIAL_CLUSTER=<%= node['etcd'][node.chef_environment]['srv01_host'] %>=https://<%= node['etcd'][node.chef_environment]['srv01_host'] %>.tabajara.intranet:2380,<%= node['etcd'][node.chef_environment]['srv02_host'] %>=https://<%= node['etcd'][node.chef_environment]['srv02_host'] %>.tabajara.intranet:2380,<%= node['etcd'][node.chef_environment]['srv03_host'] %>=https://<%= node['etcd'][node.chef_environment]['srv03_host'] %>.tabajara.intranet:2380
ETCD_TRUSTED_CA_FILE=/etc/ssl/etcd/ssl/ca.pem
ETCD_CERT_FILE=/etc/ssl/etcd/ssl/member-<%= node['etcd'][node.chef_environment]['srv01_host'] %>.pem
ETCD_KEY_FILE=/etc/ssl/etcd/ssl/member-<%= node['etcd'][node.chef_environment]['srv01_host'] %>-key.pem
ETCD_PEER_TRUSTED_CA_FILE=/etc/ssl/etcd/ssl/ca.pem
ETCD_PEER_CERT_FILE=/etc/ssl/etcd/ssl/member-<%= node['etcd'][node.chef_environment]['srv01_host'] %>.pem
ETCD_PEER_KEY_FILE=/etc/ssl/etcd/ssl/member-<%= node['etcd'][node.chef_environment]['srv01_host'] %>-key.pem
ETCD_PEER_CLIENT_CERT_AUTH=true
ETCD_AUTO_TLS="false"
ETCD_PEER_AUTO_TLS="false"
<% end -%>
