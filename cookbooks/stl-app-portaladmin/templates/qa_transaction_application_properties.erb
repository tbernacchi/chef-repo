# CDTO JDBC
spring.datasource.cdto.hikari.auto-commit=false
spring.datasource.cdto.hikari.connection-timeout=30000
spring.datasource.cdto.hikari.validation-timeout=5000
spring.datasource.cdto.hikari.idle-timeout=300000
spring.datasource.cdto.hikari.maximum-pool-size=10
spring.datasource.cdto.hikari.minimum-idle=2
spring.datasource.cdto.hikari.pool-name=HikariCP-CDTO
spring.datasource.cdto.hikari.connection-init-sql=SELECT 1 FROM DUAL
spring.datasource.cdto.hikari.max-lifetime=1800000
spring.datasource.cdto.hikari.initialization-fail-timeout=1
spring.datasource.cdto.hikari.leak-detection-threshold=10000
spring.datasource.cdto.data-source-class-name=oracle.jdbc.pool.OracleDataSource
spring.datasource.cdto.server-name=<%= node['portaladm']['qa']['datasource']['cdto']['server-name'] %>
spring.datasource.cdto.port-number=1521
spring.datasource.cdto.database-name=<%= node['portaladm']['qa']['datasource']['cdto']['db_name'] %>
spring.datasource.cdto.driver-class-name=oracle.jdbc.driver.OracleDriver
spring.datasource.cdto.url=<%= node['portaladm']['qa']['datasource']['cdto']['url'] %>
spring.datasource.cdto.username=<%= node['portaladm']['qa']['datasource']['cdto']['username'] %>
spring.datasource.cdto.password=<%= node['portaladm']['qa']['datasource']['cdto']['password'] %>
spring.datasource.cdto.type=com.zaxxer.hikari.HikariDataSource
# Oracle
spring.datasource.cdto.hikari.implicit-caching-enabled=true
spring.datasource.cdto.hikari.max-statements=250
# MySql
spring.datasource.cdto.hikari.cache-prep-stmts=true
spring.datasource.cdto.hikari.prep-stmt-cache-size=256
spring.datasource.cdto.hikari.prep-stmt-cache-sql-limit=2048
spring.datasource.cdto.hikari.use-server-prep-stmts=true
spring.datasource.cdto.hikari.rewriteBatchedStatements=true
# CDTO JPA
spring.datasource.cdto.hibernate.dialect=org.hibernate.dialect.Oracle10gDialect
spring.datasource.cdto.hibernate.show_sql=false
spring.datasource.cdto.hibernate.format_sql=true
spring.datasource.cdto.hibernate.generate_statistics=true
spring.datasource.cdto.hibernate.use_sql_comments=true
spring.datasource.cdto.hibernate.session.events.log=true
