[Unit]
Description = App validacao-cnae
After = syslog.target

[Service]
User = svc-validacao
WorkingDirectory = /app/standalone/validacao-cnae
ExecStart = /usr/bin/java -Xmx128m -Xss1m -jar -Djava.net.preferIPv4Stack=true -Dserver.port=8088 /app/standalone/validacao-cnae/validacao-cnae.jar --spring.config.location=classpath:file:/app/standalone/validacao-cnae/conf/application.properties
ExecStop = /bin/kill -s TERM $MAINPID
Restart = always
SuccessExitStatus = 143

[Install]
WantedBy = multi-user.target
