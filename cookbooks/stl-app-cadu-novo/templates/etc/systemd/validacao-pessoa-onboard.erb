[Unit]
Description = App validacao-pessoa-onboard
After = syslog.target

[Service]
User = svc-validacao
WorkingDirectory = /app/standalone/validacao-pessoa-onboard
ExecStart = /usr/bin/java -Xmx128m -Xss1m -jar -Djava.net.preferIPv4Stack=true -Dserver.port=8085 /app/standalone/validacao-pessoa-onboard/validacao-pessoa-onboard.jar --spring.config.location=classpath:file:/app/standalone/validacao-pessoa-onboard/conf/application.properties
ExecStop = /bin/kill -s TERM $MAINPID
Restart = always
SuccessExitStatus = 143

[Install]
WantedBy = multi-user.target
