description: Application access. 
context:
  application: rundeck
for:
  project:
  - allow: read
  storage:
  - allow: [read]

by:
  group: Desenvolvimento

---

description : global project config for running jobs on nodes.
context:
  project: 'TRANSACIONAL'
for:
  resource:
    - equals:
        kind: 'event'
      allow: read
    - equals:
        kind: 'node'
      allow: [read, refresh, run] # allow refresh node sources (dynamic nodes resources)
  job:
  - allow: [read, run, kill]
  node:
  - allow: [read, run]
  storage:
  - allow: [read]
by:
  group: Desenvolvimento

